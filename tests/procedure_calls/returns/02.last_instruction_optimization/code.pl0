VAR result : boolean;

PROCEDURE is_product_negative(x, y : int) -> (boolean);
	VAR result : int;

	BEGIN
		result = x * y;
		if result > 0 then begin
			return (False);
		end else begin
			return (True);
		end;

		return (False);
	END;

BEGIN
	CALL is_product_negative(-3, 4) -> (result);
	print result;
	CALL is_product_negative(3, 4) -> (result);
	print result;
END
