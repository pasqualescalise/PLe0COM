PROCEDURE multiply(x, y : int);
	VAR result : int;

	PROCEDURE square(parameter : int);
		VAR ressquare : int;
		BEGIN
			ressquare = parameter * parameter;
			print ressquare
		END;

	PROCEDURE difference(dif1, dif2 : int);
		VAR resdif : int;

		PROCEDURE sum(sum1, sum2 : int);
			VAR ressum : int;
			BEGIN
				ressum = sum1 + sum2;
				print ressum
			END;

		BEGIN
			resdif = dif1 - dif2;
			print resdif;
			CALL sum(resdif, 3)
		END;

	BEGIN
		result = x * y;
		print result;
		CALL square(result);
		CALL difference(result, x)
	END;

BEGIN
	CALL multiply(4 + 1, 2)
END
